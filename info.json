{
  "name": "vuldb",
  "label": "VulDB",
  "description": "Connector for VulDB Vulnerability and CTI API.",
  "publisher": "Community",
  "cs_approved": false,
  "cs_compatible": true,
  "version": "1.2.5",
  "category": "threatintel",
  "icon_small_name": "vuldb_small.png",
  "icon_large_name": "vuldb_large.png",
  "configuration": {
    "fields": [
      {
        "title": "API Key",
        "name": "api_key",
        "type": "password",
        "required": true,
        "editable": true,
        "visible": true,
        "tooltip": "VulDB personal API key."
      },
      {
        "title": "Base URL",
        "name": "base_url",
        "type": "text",
        "required": true,
        "editable": true,
        "visible": true,
        "value": "https://vuldb.com/?api",
        "tooltip": "VulDB API endpoint URL."
      },
      {
        "title": "API Version",
        "name": "api_version",
        "type": "text",
        "required": false,
        "editable": true,
        "visible": true,
        "value": "3",
        "tooltip": "Major API version to pin (e.g. 3)."
      },
      {
        "title": "Verify SSL",
        "name": "verify_ssl",
        "type": "checkbox",
        "required": false,
        "editable": true,
        "visible": true,
        "value": true,
        "tooltip": "Verify SSL certificate of the VulDB endpoint."
      },
      {
        "title": "Skip API Health Check",
        "name": "skip_health_check_api_call",
        "type": "checkbox",
        "required": false,
        "editable": true,
        "visible": true,
        "value": true,
        "tooltip": "If enabled, health check only validates API key presence (no API call)."
      }
    ]
  },
